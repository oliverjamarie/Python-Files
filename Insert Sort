import random

size = 50
list = []




for i in range (size) :
    x = random.randint (1,20)
    list.append(x)

print list

def insertSort (array) :
        
    for i in range(1,size):
        x = array[i]
        j = i
        while j > 0 and array[j-1] > x:
            array[j] = array [j-1]
            j -=1
        array[j] = x

insertSort (list)

print list
